  author = {Hansen, Steen Laugesen and Ray, Peter Martin and Karlsson, Anders Ola and J{\o}rgensen, Bodil and Borkhardt, Bernhard and Petersen, Bent Larsen and Ulvskov, Peter},
  month = jan,
  year = {2011},
  pages = {246-258},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\JY3N3NYC\\Hansen et al. - 2011 - Mechanical Properties of Plant Cell Walls Probed b.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\78IXSK9X\\246.html},
