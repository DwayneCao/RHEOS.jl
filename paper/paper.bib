  author = {Popper, Zo{\"e} A. and Michel, Gurvan and Herv{\'e}, C{\'e}cile and Domozych, David S. and Willats, William G.T. and Tuohy, Maria G. and Kloareg, Bernard and Stengel, Dagmar B.},
  month = jun,
  year = {2011},
  pages = {567-590},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\YI5DKUH4\\Popper et al. - 2011 - Evolution and Diversity of Plant Cell Walls From .pdf}
