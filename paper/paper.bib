@article{mainardiPropertiesMittagLefflerFunction2014,
  title = {On Some Properties of the {{Mittag}}-{{Leffler}} Function {{E}}{\textsubscript{{$\alpha$}}}(-{{t}}{\textsuperscript{{$\alpha$}}}), Completely Monotone for {{t$>$0}} with 0{$<\alpha{}<$}1},
  volume = {19},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  abstract = {We analyse some peculiar properties of the function
of the Mittag-Leffler (M-L) type,
\$e\_\textbackslash{}alpha(t) := E\_\textbackslash{}alpha(-t\^\textbackslash{}alpha)\$  for \$0{$<\backslash$}alpha{$<$}1\$ and \$t{$>$}0\$, which  is known to
 be completely monotone (CM)  with a non-negative spectrum of frequencies and times,
suitable to model fractional relaxation processes.
 We first note that (surprisingly)  these two spectra coincide so providing
a universal scaling  property of  this function, not  well pointed out in the literature.
Furthermore, we consider  the problem of approximating our  M-L  function  with simpler CM  functions for small and large times.
We provide two different sets of elementary CM functions that are asymptotically  equivalent to \$e\_\textbackslash{}alpha(t)\$
as \$t\textbackslash{}to 0\$ and \$t\textbackslash{}to +\textbackslash{}infty\$.
The first set is given by the stretched exponential for small times
and the power law for large times, following a standard approach.
For the second set we chose  two rational CM functions in \$t\^\textbackslash{}alpha\$,
obtained as the  Pad\textbackslash{}`e  Approximants  (PA)  \$[0/1]\$  to the convergent series in positive powers  (as \$t\textbackslash{}to 0\$)
and to the asymptotic series in negative powers  (as \$t\textbackslash{}to \textbackslash{}infty\$),  respectively.
From numerical computations we are allowed to the conjecture that the second set provides upper and lower bounds to the Mittag-Leffler function.},
  language = {en},
  urldate = {2018-08-21},
  journal = {Discrete \& Continuous Dynamical Systems},
  doi = {10.3934/dcdsb.2014.19.2267},
