@article{shenViscoelasticPropertiesIsolated2011,
  title = {Viscoelastic {{Properties}} of {{Isolated Collagen Fibrils}}},
  volume = {100},
  issn = {0006-3495},
  abstract = {Understanding the viscoelastic behavior of collagenous tissues with complex hierarchical structures requires knowledge of the properties at each structural level. Whole tissues have been studied extensively, but less is known about the mechanical behavior at the submicron, fibrillar level. Using a microelectromechanical systems platform, in~vitro coupled creep and stress relaxation tests were performed on collagen fibrils isolated from the sea cucumber dermis. Stress-strain-time data indicate that isolated fibrils exhibit viscoelastic behavior that could be fitted using the Maxwell-Weichert model. The fibrils showed an elastic modulus of 123 {$\pm$} 46 MPa. The time-dependent behavior was well fit using the two-time-constant Maxwell-Weichert model with a fast time response of 7 {$\pm$} 2~s and a slow time response of 102 {$\pm$} 5 s. The fibrillar relaxation time was smaller than literature values for tissue-level relaxation time, suggesting that tissue relaxation is dominated by noncollagenous components (e.g., proteoglycans). Each specimen was tested three times, and the only statistically significant difference found was that the elastic modulus is larger in the first test than in the subsequent two tests, indicating that viscous properties of collagen fibrils are not sensitive to the history of previous tests.},
  number = {12},
  urldate = {2018-09-02},
  journal = {Biophysical Journal},
  doi = {10.1016/j.bpj.2011.04.052},
  url = {http://www.sciencedirect.com/science/article/pii/S0006349511005625},
  author = {Shen, Zhilei Liu and Kahn, Harold and Ballarini, Roberto and Eppell, Steven J.},
  month = jun,
  year = {2011},
  pages = {3008-3015},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\6QUK3EZY\\Shen et al. - 2011 - Viscoelastic Properties of Isolated Collagen Fibri.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\MZGFKT22\\S0006349511005625.html}
}

@article{mizumotoOptimizingMatingEncounters2017,
