  keywords = {Cell mechanics,Plant cell wall,Cell growth,Micro-indentation,Polar growth,Pollen tube,Tip growth,Turgor},
  pages = {437-446},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\FY5VWDXN\\Zerzour et al. - 2009 - Polar growth in pollen tubes is associated with sp.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\VFN9ETLL\\S0012160609011099.html}
