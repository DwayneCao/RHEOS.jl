  urldate = {2019-04-05},
  journal = {bioRxiv},
  doi = {10.1101/565614},
  url = {https://www.biorxiv.org/content/10.1101/565614v1},
  author = {Kaplan, J. L. and Torode, T. A. and Daher, F. Bou and Braybrook, S. A.},
  month = mar,
  year = {2019},
  pages = {565614},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\XEDN5DIK\\Kaplan et al. - 2019 - On Pectin Methyl-esterification Implications for .pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\SR2BY9M2\\565614v1.html}
