  month = feb,
  year = {2018},
  pages = {12},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\5WXEZK6N\\Ostoja-Starzewski and Zhang - 2018 - Does a Fractal Microstructure Require a Fractional.pdf}
}

