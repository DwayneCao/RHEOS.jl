@article{francisSeparationArabidopsisPollen2006,
  title = {Separation of {{Arabidopsis Pollen Tetrads Is Regulated}} by {{QUARTET1}}, a {{Pectin Methylesterase Gene}}},
  volume = {142},
  copyright = {\textcopyright{} 2006 American Society of Plant Biologists},
  issn = {0032-0889, 1532-2548},
  abstract = {Arabidopsis (Arabidopsis thaliana) QUARTET (QRT) genes are required for pollen separation during normal floral development. In qrt mutants, the four products of microsporogenesis remain fused and pollen grains are released as tetrads. In Arabidopsis, tetrad analysis in qrt mutants has been used to map all five centromeres, easily distinguish sporophytic from gametophytic mutations, and accurately assess crossover interference. Using a combination of forward and reverse genetics, we have identified the gene responsible for the qrt1 phenotype. Annotation predicts that QRT1 encodes a pectin methylesterase (PME), and enzymatic assays of QRT1 expressed in Escherichia coli indicate that QRT1 has PME activity. Promoter and transcription analysis demonstrate QRT1 is expressed in anther tissues shortly after meiosis is complete. Unexpectedly, the QRT1 promoter is also active in a variety of developmentally unrelated tissues, including developing guard cells, the hypocotyl-root transition zone, areas of lateral root emergence, and floral nectaries. PMEs constitute a large gene family in Arabidopsis, are involved in cell wall loosening, and have been implicated in various aspects of floral development and pollen tube elongation. The identification of QRT1 as a PME contributes to our understanding of pollen development and may help to provide valuable genetic tools in other plant species.},
  language = {en},
  number = {3},
  urldate = {2019-01-03},
  journal = {Plant Physiology},
  doi = {10.1104/pp.106.085274},
  url = {http://www.plantphysiol.org/content/142/3/1004},
  author = {Francis, Kirk E. and Lam, Sandy Y. and Copenhaver, Gregory P.},
  month = nov,
  year = {2006},
  pages = {1004-1013},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\FVYWAKJN\\Francis et al. - 2006 - Separation of Arabidopsis Pollen Tetrads Is Regula.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\WQJ4Q5DQ\\1004.html},
