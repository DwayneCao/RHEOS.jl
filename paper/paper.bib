  author = {Gigli, Jiabril and Garnier, Catherine and Piazza, Laura},
  month = jul,
  year = {2009},
  pages = {1406-1412},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\9VGG8KA8\\Gigli et al. - 2009 - Rheological behaviour of low-methoxyl pectin gels .pdf}
