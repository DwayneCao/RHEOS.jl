  year = {2014},
  keywords = {Creep,Convolution,Relaxation,Boltzmann,Constitutive,Cycle,Equilibrium,Fading,Glassy,Hereditary,Isothermal,Laplace,Long-term,Matrix,Memory,Operator,Principle,Symbolic},
  pages = {23-52},
