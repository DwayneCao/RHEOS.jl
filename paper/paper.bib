  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ptr.1953},
  author = {Eliaz, Isaac and Hotchkiss, Arland T. and Fishman, Marshall L. and Rode, Dorena},
  year = {2006},
  keywords = {chelation,arsenic,heavy metals,modified citrus pectin,PectaSol,rhamnogalacturonan},
  pages = {859-864},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\KFDSCQSA\\Eliaz et al. - 2006 - The effect of modified citrus pectin on urinary ex.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\X6K8R927\\ptr.html}
