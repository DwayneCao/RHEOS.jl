  month = apr,
  year = {2016},
  pages = {44-50},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\IB7MVG2J\\Li et al. - 2016 - Viscoelastic-plastic behavior of single tomato mes.pdf}
}

