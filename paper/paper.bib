  author = {Milani, Pascale and Gholamirad, Maryam and Traas, Jan and Arn{\'e}odo, Alain and Boudaoud, Arezki and Argoul, Fran{\c c}oise and Hamant, Olivier},
  month = sep,
  year = {2011},
  keywords = {atomic force microscopy,biomechanics,cell wall,meristem,morphogenesis,stiffness},
  pages = {1116-1123},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\BA3BEU4C\\Milani et al. - 2011 - In vivo analysis of local wall stiffness at the sh.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\FTG88CNM\\j.1365-313X.2011.04649.html}
