  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\4UVIH2N3\\Zhang et al. - 2018 - Characteristics of pectin from black cherry tomato.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\HZ28LUIK\\S0260877417303308.html}
