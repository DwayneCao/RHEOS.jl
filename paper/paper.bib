  url = {http://www.sciencedirect.com/science/article/pii/S0268005X1500260X},
  author = {Lin, Dehui and {Lopez-Sanchez}, Patricia and Gidley, Michael J.},
  month = jan,
  year = {2016},
  keywords = {Pectin,Cellulose,Binding,De-binding,Mechanical compression},
  pages = {57-68},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\8T82GR9H\\Lin et al. - 2016 - Interactions of pectins with cellulose during its .pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\Z9SIQB63\\S0268005X1500260X.html}
