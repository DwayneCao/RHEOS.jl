  month = feb,
  year = {2012},
  keywords = {Pectin,De-esterification,Absolute degree of blockiness,Ca-pectin gels,âEgg-boxâ model,Gel stiffness},
  pages = {69-76},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\EQJXYHRV\\NgouÃ©mazong et al. - 2012 - Stiffness of Ca2+-pectin gels combined effects of.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\ZWT43NTN\\S0008621511005581.html}
