@article{ostoja-starzewskiDoesFractalMicrostructure2018,
  title = {Does a {{Fractal Microstructure Require}} a {{Fractional Viscoelastic Model}}?},
  volume = {2},
  issn = {2504-3110},
  abstract = {The question addressed by this paper is tackled through a continuum micromechanics model of a 2D random checkerboard, in which one phase is linear elastic and another linear viscoelastic of integer-order. The spatial homogeneity and ergodicity of the material statistics justify homogenization in the vein of the Hill\textendash{}Mandel condition for viscoelastic media. Thus, uniform kinematic- or traction-controlled boundary conditions, applied to sufficiently large domains, provide macroscopic (RVE level) responses. With computational mechanics, this strategy is applied over the entire range of the relative content of both phases. Setting the volume fraction of either the elastic phase or the viscoelastic phase at the critical value ( 0.59) results in fractal patterns of site-percolation. Extensive simulations of boundary value problems show that, for a viscoelastic composite having such a fractal structure, the integer (not fractional) calculus model is adequate. In other words, the spatial randomness of the composite material\textemdash{}even in the fractal regime\textemdash{}is not necessarily the cause of the fractional order viscoelasticity.},
  language = {en},
  number = {1},
  urldate = {2019-03-03},
  journal = {Fractal and Fractional},
  doi = {10.3390/fractalfract2010012},
  url = {http://www.mdpi.com/2504-3110/2/1/12},
