  author = {Di Paola, Mario and Fiore, Vincenzo and Pinnola, Francesco Paolo and Valenza, Antonino},
  month = feb,
  year = {2014},
  keywords = {Viscoelasticity,Fractional calculus,Relaxation test},
  pages = {63-70},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\3SY33PPD\\Di Paola et al. - 2014 - On the influence of the initial ramp for a correct.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\3P5WNLDR\\S0167663613002081.html}
