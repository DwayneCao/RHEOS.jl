  author = {Mouille, Gr{\'e}gory and Ralet, Marie-Christine and Cavelier, C{\'e}line and Eland, Cathlene and Effroy, Delphine and H{\'e}maty, Kian and McCartney, Lesley and Truong, Hoai Nam and Gaudon, Virginie and Thibault, Jean-Fran{\c c}ois and Marchant, Alan and H{\"o}fte, Herman},
  year = {2007},
  keywords = {pectin,Arabidopsis,homogalacturonan,methyltransferase,plant cell wall},
  pages = {605-614},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\RRV5PCPA\\Mouille et al. - 2007 - Homogalacturonan synthesis in Arabidopsis thaliana.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\WBHLDKPX\\j.1365-313X.2007.03086.html}
