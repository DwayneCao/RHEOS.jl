  month = nov,
  year = {2002},
  keywords = {Gelation,Rheological properties,Amide pectin,Viscoelastic behaviour},
  pages = {123-129},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\6P2CJHIB\\Alonso-MougÃ¡n et al. - 2002 - Rheological behaviour of an amide pectin.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\DNBHUP3R\\S0260877402000262.html}
