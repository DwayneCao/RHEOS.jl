@article{colin-yorkFutureTractionForce2018,
  series = {Futures of {{Biomedical Engineering}}: {{Cardiovascular Bioengineering}} and {{Vascular Biomechanics}}},
  title = {The Future of Traction Force Microscopy},
  volume = {5},
  issn = {2468-4511},
  abstract = {Animal cells continuously sense and respond to mechanical force. Quantifying these forces remains a major challenge in bioengineering; yet such measurements are essential for the understanding of cellular function. Traction force microscopy is one of the most successful and broadly-used force probing technologies, chosen for the simplicity of its implementation, flexibility to mimic cellular conditions, and well-established analysis pipe-line. Here, we review the accomplishments, and discuss the applicability and limitations of traction force microscopy. We explain fundamental shortcomings of the method, summarise latest improvements, and outline future pathways towards the impact of the method, especially considering latest developments in state-of-the-art super-resolution fluorescence imaging. In light of the increasing discovery of the importance of mechanobiology in cell physiology, we envisage traction force microscopy to remain a major player for quantifying mechanical forces in living cells.},
  urldate = {2019-03-24},
  journal = {Current Opinion in Biomedical Engineering},
  doi = {10.1016/j.cobme.2017.10.002},
  url = {http://www.sciencedirect.com/science/article/pii/S2468451117300727},
  author = {{Colin-York}, Huw and Fritzsche, Marco},
  month = mar,
  year = {2018},
  keywords = {Cell mechanics,Actin cytoskeleton,Mechanobiology,Traction force microscopy},
  pages = {1-5},
