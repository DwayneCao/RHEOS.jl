  keywords = {pectin,cell wall,guard cell,stomata},
  pages = {2899-2906},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\7SUZYZNZ\\Amsbury et al. - 2016 - Stomatal Function Requires Pectin De-methyl-esteri.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\EZK8VC9B\\S0960982216309332.html}
