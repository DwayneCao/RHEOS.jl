  number = {8},
  urldate = {2018-12-21},
  journal = {Magnetic Resonance in Chemistry},
  doi = {10.1002/mrc.3836},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mrc.3836},
  author = {Dick-Perez, Marilu and Wang, Tuo and Salazar, Andre and Zabotina, Olga A. and Hong, Mei},
  year = {2012},
  keywords = {pectic polysaccharides,solid-state NMR},
  pages = {539-550},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\5CVEFDRL\\DickâPerez et al. - 2012 - Multidimensional solid-state NMR studies of the st.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\URT44SGZ\\mrc.html}
}

@article{sassiAuxinMediatedShiftGrowth2014,
  title = {An {{Auxin}}-{{Mediated Shift}} toward {{Growth Isotropy Promotes Organ Formation}} at the {{Shoot Meristem}} in {{Arabidopsis}}},
  volume = {24},
  issn = {0960-9822},
  abstract = {Summary
To control morphogenesis, molecular regulatory networks have to interfere with the mechanical properties of the individual cells of developing organs and tissues, but how this is achieved is not well known. We study this issue here in the shoot meristem of higher plants, a group of undifferentiated cells where complex changes in growth rates and directions lead to the continuous formation of new organs [1, 2]. Here, we show that the plant hormone auxin plays an important role in this process via a dual, local effect on the extracellular matrix, the cell wall, which determines cell shape. Our study reveals that auxin not only causes a limited reduction in wall stiffness but also directly interferes with wall anisotropy via the regulation of cortical microtubule dynamics. We further show that to induce growth isotropy and organ outgrowth, auxin somehow interferes with the cortical microtubule-ordering activity of a network of proteins, including AUXIN BINDING PROTEIN 1 and KATANIN 1. Numerical simulations further indicate that the induced isotropy is sufficient to amplify the effects of the relatively minor changes in wall stiffness to promote organogenesis and the establishment of new growth axes in a robust manner.},
  number = {19},
  urldate = {2018-12-21},
  journal = {Current Biology},
  doi = {10.1016/j.cub.2014.08.036},
  url = {http://www.sciencedirect.com/science/article/pii/S0960982214010495},
  author = {Sassi, Massimiliano and Ali, Olivier and Boudon, Fr{\'e}d{\'e}ric and Cloarec, Gladys and Abad, Ursula and Cellier, Coralie and Chen, Xu and Gilles, Benjamin and Milani, Pascale and Friml, Ji{\v r}{\'i} and Vernoux, Teva and Godin, Christophe and Hamant, Olivier and Traas, Jan},
  month = oct,
  year = {2014},
  pages = {2335-2342},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\TISGX26V\\Sassi et al. - 2014 - An Auxin-Mediated Shift toward Growth Isotropy Pro.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\IILGCEMW\\S0960982214010495.html}
