  month = nov,
  year = {2003},
  pages = {801-807},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\5U7QC8GQ\\Cardoso et al. - 2003 - Temperature dependence of the formation and meltin.pdf}
}

