  pages = {e0122132},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\RR5HB74T\\Lopez-Sanchez et al. - 2015 - Poroelastic Mechanical Effects of Hemicelluloses o.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\TFKQWDSV\\article.html}
