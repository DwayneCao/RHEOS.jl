  month = mar,
  year = {2018},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\WWF3ZALI\\Iszatt-White et al. - 2018 - The âCorbyn Phenomenonâ Media Representations of .pdf}
}

@article{posnanskyFractalNetworkDimension2012,
