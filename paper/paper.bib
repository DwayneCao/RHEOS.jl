  keywords = {Pectin,Cellulose,Mechanics,SANS,SAXS,XRD},
  pages = {71-81},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\BSX7BERB\\Lopez-Sanchez et al. - 2017 - Cellulose-pectin composite hydrogels Intermolecul.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\BYLVXRHG\\S0144861717300607.html}
