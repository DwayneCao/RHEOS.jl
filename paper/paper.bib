@article{mainardiGeneralizationBeckerModel2018,
  title = {A Generalization of the {{Becker}} Model in Linear Viscoelasticity: Creep, Relaxation and Internal Friction},
  issn = {1385-2000, 1573-2738},
  shorttitle = {A Generalization of the {{Becker}} Model in Linear Viscoelasticity},
  abstract = {We present a new rheological model depending on a real parameter {$\nu$} {$\in$} [0, 1], which reduces to the Maxwell body for {$\nu$} = 0 and to the Becker body for {$\nu$} = 1. The corresponding creep law is expressed in an integral form in which the exponential function of the Becker model is replaced and generalized by a Mittag\textendash{}Leffler function of order {$\nu$}. Then the corresponding non-dimensional creep function and its rate are studied as functions of time for different values of {$\nu$} in order to visualize the transition from the classical Maxwell body to the Becker body. Based on the hereditary theory of linear viscoelasticity, we also approximate the relaxation function by solving numerically a Volterra integral equation of the second kind. In turn, the relaxation function is shown versus time for different values of {$\nu$} to visualize again the transition from the classical Maxwell body to the Becker body. Furthermore, we provide a full characterization of the new model by computing, in addition to the creep and relaxation functions, the so-called specific dissipation Q-1 as a function of frequency, which is of particular relevance for geophysical applications.},
  language = {en},
  urldate = {2018-08-27},
  journal = {Mechanics of Time-Dependent Materials},
  doi = {10.1007/s11043-018-9381-4},
  url = {http://link.springer.com/10.1007/s11043-018-9381-4},
  author = {Mainardi, Francesco and Masina, Enrico and Spada, Giorgio},
  month = feb,
  year = {2018},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\WL4FAZD4\\Mainardi et al. - 2018 - A generalization of the Becker model in linear vis.pdf}
}

@article{lublinerModifiedKuhnModel1992,
