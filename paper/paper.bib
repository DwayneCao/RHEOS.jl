  url = {http://stacks.iop.org/0031-9155/57/i=12/a=4023?key=crossref.20ee0e8e2fa92b77c8726bb87bb4cead},
  author = {Posnansky, Oleg and Guo, Jing and Hirsch, Sebastian and Papazoglou, Sebastian and Braun, J{\"u}rgen and Sack, Ingolf},
  month = jun,
  year = {2012},
