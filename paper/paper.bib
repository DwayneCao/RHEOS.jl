  author = {Croser, Clare and Bengough, A. Glyn and Pritchard, Jeremy},
  year = {2000},
  pages = {150-159},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\RIZKUDF5\\Croser et al. - 2000 - The effect of mechanical impedance on root growth .pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\M9CBHBZE\\j.1399-3054.2000.100207.html}
}

