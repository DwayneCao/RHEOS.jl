  keywords = {Creep,Stress relaxation,Cell wall,Expansin,Extensibility},
  pages = {291-303},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\FMFCM2EX\\Cosgrove - 2011 - Measuring In Vitro Extensibility of Growing Plant .pdf},
  doi = {10.1007/978-1-61779-008-9_20}
}

@article{derbyshireRestrictedCellElongation2007,
  title = {Restricted Cell Elongation in {{Arabidopsis}} Hypocotyls Is Associated with a Reduced Average Pectin Esterification Level},
  volume = {7},
  issn = {14712229},
  abstract = {Background: Cell elongation is mainly limited by the extensibility of the cell wall. Dicotyledonous primary (growing) cell walls contain cellulose, xyloglucan, pectin and proteins, but little is known about how each polymer class contributes to the cell wall mechanical properties that control extensibility.
Results: We present evidence that the degree of pectin methyl-esterification (DE\%) limits cell growth, and that a minimum level of about 60\% DE is required for normal cell elongation in Arabidopsis hypocotyls. When the average DE\% falls below this level, as in two gibberellic acid (GA) mutants ga1-3 and gai, and plants expressing pectin methyl-esterase (PME1) from Aspergillus aculeatus, then hypocotyl elongation is reduced.
Conclusion: Low average levels of pectin DE\% are associated with reduced cell elongation, implicating PMEs, the enzymes that regulate DE\%, in the cell elongation process and in responses to GA. At high average DE\% other components of the cell wall limit GA-induced growth.},
  language = {en},
  number = {1},
  urldate = {2018-12-23},
  journal = {BMC Plant Biology},
  doi = {10.1186/1471-2229-7-31},
  url = {http://bmcplantbiol.biomedcentral.com/articles/10.1186/1471-2229-7-31},
  author = {Derbyshire, Paul and McCann, Maureen C and Roberts, Keith},
  year = {2007},
  pages = {31},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\FVV8XGXU\\Derbyshire et al. - 2007 - Restricted cell elongation in Arabidopsishypocotyl.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\YYR8WWXH\\Derbyshire et al. - 2007 - Restricted cell elongation in Arabidopsis hypocoty.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\MG5YG6DD\\1471-2229-7-31.html}
}

@article{pelletierRolePectinDemethylesterification2010,
  title = {A Role for Pectin De-Methylesterification in a Developmentally Regulated Growth Acceleration in Dark-Grown {{Arabidopsis}} Hypocotyls},
  volume = {188},
  issn = {1469-8137},
  abstract = {\textbullet{} We focused on a developmentally regulated growth acceleration in the dark-grown Arabidopsis hypocotyl to study the role of changes in cell wall metabolism in the control of cell elongation. \textbullet{} To this end, precise transcriptome analysis on dissected dark-grown hypocotyls, Fourier transform infrared (FT-IR) microspectroscopy and kinematic analysis were used. \textbullet{} Using a cellulose synthesis inhibitor, we showed that the growth acceleration marks a developmental transition during which growth becomes uncoupled from cellulose synthesis. We next investigated the cellular changes that take place during this transition. FT-IR microspectroscopy revealed significant changes in cell wall composition during, but not after, the growth acceleration. Transcriptome analysis suggested a role for cell wall remodeling, in particular pectin modification, in this growth acceleration. This was confirmed by the overexpression of a pectin methylesterase inhibitor, which caused a delay in the growth acceleration. \textbullet{} This study shows that the acceleration of cell elongation marks a developmental transition in dark-grown hypocotyl cells and supports a role for pectin de-methylesterification in the timing of this event.},
  language = {en},
  number = {3},
  urldate = {2018-12-23},
  journal = {New Phytologist},
  doi = {10.1111/j.1469-8137.2010.03409.x},
  url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/j.1469-8137.2010.03409.x},
  author = {Pelletier, Sandra and Orden, J{\"u}rgen Van and Wolf, Sebastian and Vissenberg, Kris and Delacourt, Julien and Ndong, Yves Assoumou and Pelloux, J{\'e}r{\^o}me and Bischoff, Volker and Urbain, Aur{\'e}lie and Mouille, Gr{\'e}gory and Lemonnier, Ga{\"e}tan and Renou, Jean-Pierre and H{\"o}fte, Herman},
  year = {2010},
  keywords = {cellulose,hypocotyl,cell wall,Arabidopsis thaliana,cell elongation,Fourier transform infrared (FT-IR) microspectroscopy,pectin de-methylesterification,transcriptome},
  pages = {726-739},
  file = {C:\\Users\\louis.kaplan\\Zotero\\storage\\3UPVVRDF\\Pelletier et al. - 2010 - A role for pectin de-methylesterification in a dev.pdf;C:\\Users\\louis.kaplan\\Zotero\\storage\\IT6SN2W7\\j.1469-8137.2010.03409.html}
}

@article{mouilleHomogalacturonanSynthesisArabidopsis2007,
  title = {Homogalacturonan Synthesis in {{Arabidopsis}} Thaliana Requires a {{Golgi}}-Localized Protein with a Putative Methyltransferase Domain},
  volume = {50},
  issn = {1365-313X},
  abstract = {Pectins are a family of complex cell-wall polysaccharides, the biosynthesis of which remains poorly understood. We identified dwarf mutants with reduced cell adhesion at a novel locus, QUASIMODO2 (QUA2). qua2-1 showed a 50\% reduction in homogalacturonan (HG) content compared with the wild type, without affecting other cell-wall polysaccharides. The remaining HG in qua2-1 showed an unaltered degree of methylesterification. Positional cloning and GFP fusions showed that QUA2, consistent with a role in HG synthesis, encodes a Golgi-localized protein. In contrast to QUA1, another Golgi-localized protein required for HG-synthesis, QUA2 does not show sequence similarity to glycosyltransferases, but instead contains a putative methyltransferase (MT) domain. The Arabidopsis genome encodes 29 QUA2-related proteins. Interestingly, the transcript profiles of QUA1 and QUA2 are correlated and other pairs of QUA1 and QUA2 homologues with correlated transcript profiles can be identified. Together, the results lead to the hypothesis that QUA2 is a pectin MT, and that polymerization and methylation of homogalacturonan are interdependent reactions.},
  language = {en},
  number = {4},
  urldate = {2018-12-22},
  journal = {The Plant Journal},
  doi = {10.1111/j.1365-313X.2007.03086.x},
